PORT=4000
GEMINI_API_KEY=your_gemini_api_key_here

# MinIO connection
MINIO_ENDPOINT=localhost
MINIO_PORT=9000
MINIO_USE_SSL=false
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=forms

# Rate Limiting
# Global rate limit window in milliseconds (default: 15 minutes)
RATE_LIMIT_GLOBAL_WINDOW_MS=900000
# Global max requests per window (default: 100)
RATE_LIMIT_GLOBAL_MAX=100
# LLM rate limit window in milliseconds (default: 1 minute)
RATE_LIMIT_LLM_WINDOW_MS=60000
# LLM max requests per window (default: 10)
RATE_LIMIT_LLM_MAX=10

# Input Validation Limits
# Max length for question input (default: 1000)
INPUT_MAX_LENGTH_QUESTION=1000
# Max length for value inputs (default: 10000)
INPUT_MAX_LENGTH_VALUE=10000
# Max length for attribute name (default: 200)
INPUT_MAX_LENGTH_ATTRIBUTE_NAME=200
# Max length for attribute value (default: 10000)
INPUT_MAX_LENGTH_ATTRIBUTE_VALUE=10000
# Max number of examples allowed (default: 10)
INPUT_MAX_EXAMPLES_COUNT=10
# Max length for each example (default: 1000)
INPUT_MAX_LENGTH_EXAMPLE=1000
